{
  "name": "@maktouch/devctl",
  "version": "5.0.0",
  "contributors": [
    "Makara Sok",
    "Enrico Suarez",
    "Rokn Hamdan"
  ],
  "description": "Easily start developing in monorepos with docker-compose",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "devctl": "./bin/run.js"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist/",
    "dist": "tsc --sourceMap false",
    "prepare": "pnpm run clean && pnpm run dist",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "pnpm run build && oclif manifest"
  },
  "oclif": {
    "bin": "devctl",
    "dirname": "devctl",
    "commands": "./dist/commands",
    "plugins": [],
    "hooks": {
      "command_not_found": "./dist/hooks/command_not_found"
    },
    "topicSeparator": " ",
    "topics": {}
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "readme.md",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maktouch/devctl.git"
  },
  "bugs": {
    "url": "https://github.com/maktouch/devctl/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@expo/spawn-async": "^1.7.2",
    "@google-cloud/secret-manager": "^6.1.1",
    "@oclif/core": "^4.8.0",
    "bluebird": "^3.7.2",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "concurrently": "^9.2.1",
    "cosmiconfig": "^9.0.0",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.2.3",
    "ejs": "^4.0.1",
    "inquirer": "^13.2.2",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23",
    "mustache": "^4.2.0",
    "node_modules-path": "^2.3.0",
    "oclif": "^4.22.73",
    "strip-ansi": "^7.1.2"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.16",
    "@types/bluebird": "^3.5.42",
    "@types/ejs": "^3.1.5",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.23",
    "@types/mustache": "^4.2.6",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=16.18.1"
  }
}
